  "networkMode": "bridge",
  "status": "ACTIVE",
  "containerDefinitions": [
    {
      "volumesFrom": [],
      "portMappings": [],
      "essential": true,
      "entryPoint": [
        "/bin/bash",
        "-c"
      ],
      "mountPoints": [
        {
          "containerPath": "/root/DVA/dva/media",
          "sourceVolume": "dvadata",
          "readOnly": false
        },
        {
          "containerPath": "/usr/local/nvidia",
          "sourceVolume": "nvidia",
          "readOnly": false
        }
      ],
      "name": "gpu",
      "environment": [
        {
          "name": "BROKER_URL",
          "value": ""
        },
        {
          "name": "CELERY_MUTE",
          "value": "1"
        },
        {
          "name": "DATABASE_URL",
          "value": ""
        },
        {
          "name": "DOCKER_MODE",
          "value": "1"
        },
        {
          "name": "GPU_AVAILABLE",
          "value": "1"
        },
        {
          "name": "LAUNCH_BY_NAME_analyzer_crnn",
          "value": "1"
        },
        {
          "name": "LAUNCH_BY_NAME_analyzer_tagger",
          "value": "1"
        },
        {
          "name": "LAUNCH_BY_NAME_detector_coco",
          "value": "1"
        },
        {
          "name": "LAUNCH_BY_NAME_detector_face",
          "value": "1"
        },
        {
          "name": "LAUNCH_BY_NAME_indexer_facenet",
          "value": "1"
        },
        {
          "name": "LAUNCH_BY_NAME_indexer_inception",
          "value": "1"
        },
        {
          "name": "MEDIA_BUCKET",
          "value": ""
        },
        {
          "name": "SECRET_KEY",
          "value": ""
        }
      ],
      "image": "akshayubhat/dva-auto:gpu",
      "command": [
        "youtube-dl -U && git pull && fab start_container:worker"
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "dva_ecs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "dva_cpu_"
        }
      },
      "cpu": 4000,
      "privileged": true,
      "memoryReservation": 32000
    }
  ],
  "placementConstraints": [],
  "volumes": [
    {
      "host": {
        "sourcePath": "/efs/"
      },
      "name": "dvadata"
    },
    {
      "host": {
        "sourcePath": "/var/lib/nvidia-docker/volumes/nvidia_driver/latest"
      },
      "name": "nvidia"
    }
  ],
  "family": "DeepVideoAnalyticsGPU"
